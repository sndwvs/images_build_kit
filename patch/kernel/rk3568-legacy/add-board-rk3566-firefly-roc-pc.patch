diff -Naur a/arch/arm64/boot/dts/rockchip/rk3566-firefly-roc-pc-cam-8ms1m.dtsi b/arch/arm64/boot/dts/rockchip/rk3566-firefly-roc-pc-cam-8ms1m.dtsi
