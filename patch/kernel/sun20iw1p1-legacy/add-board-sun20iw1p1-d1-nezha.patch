+		CPU0: cpu@0 {
+			device_type = "cpu";
+			reg = <0>;
+			status = "okay";
+			compatible = "riscv";
+			riscv,isa = "rv64imafdcvsu";
+		/*	riscv,priv-major = <1>;*/
+		/*	riscv,priv-minor = <10>;*/
+			mmu-type = "riscv,sv39";
+			clocks = <&ccu CLK_RISCV>;
+			clock-frequency = <24000000>;
+			operating-points-v2 = <&cpu_opp_table>;
+			cpu-idle-states = <&CPU_SLEEP>;
