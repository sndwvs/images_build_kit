+	hdmi_mode_check          = <1>;
+
+	fb0_format               = <0>;
+	fb0_width                = <1280>;
+	fb0_height               = <720>;
+
+	fb1_format               = <0>;
+	fb1_width                = <1280>;
+	fb1_height               = <720>;
