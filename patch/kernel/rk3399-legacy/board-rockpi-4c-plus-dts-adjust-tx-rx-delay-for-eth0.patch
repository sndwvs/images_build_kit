From 42093936ccdec879291bc6c7060e99e330384ffc Mon Sep 17 00:00:00 2001
From: Ken <ken@radxa.com>
Date: Mon, 1 Aug 2022 10:11:05 +0800
Subject: [PATCH] arm64: dts: rockpi-4cplus: adjust tx/rx delay for eth0

Signed-off-by: Ken <ken@radxa.com>
---
 arch/arm64/boot/dts/rockchip/rk3399-rock-pi-4c-plus.dts | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/arch/arm64/boot/dts/rockchip/rk3399-rock-pi-4c-plus.dts b/arch/arm64/boot/dts/rockchip/rk3399-rock-pi-4c-plus.dts
index 8fae0322b5339..d05316dd40e05 100644
--- a/arch/arm64/boot/dts/rockchip/rk3399-rock-pi-4c-plus.dts
+++ b/arch/arm64/boot/dts/rockchip/rk3399-rock-pi-4c-plus.dts
@@ -784,8 +784,8 @@
 	pinctrl-names = "default", "sleep";
 	pinctrl-0 = <&rgmii_pins>;
 	pinctrl-1 = <&rgmii_sleep_pins>;
-	tx_delay = <0x28>;
-	rx_delay = <0x11>;
+	tx_delay = <0x2a>;
+	rx_delay = <0x21>;
 	status = "okay";
 };
 
